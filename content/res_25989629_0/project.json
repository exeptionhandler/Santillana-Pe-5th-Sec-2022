      "ab9f78319d3f7507fe0e5b2640f39457": {
        "id": "ab9f78319d3f7507fe0e5b2640f39457",
        "name": "text",
        "htmlDataRoot": false,
        "readOnly": false,
        "parent": "ac7e1ad70a4349956979c754e39243b5",
        "properties": {
          "view": "text_view_529-001",
          "look": "text_view_529-001_look_001",
          "liColor": "#f54528",
          "textContent": "<ul><li><span style=\"color:#6e6e6e;\">Fortalecer las virtudes del compromiso y la responsabilidad.</span></li><li><span style=\"color:#6e6e6e;\"><span style=\"font-size: 1.125rem;\">Elaborar un cuaderno de itinerario para descubrir el llamado que nos hace Dios para construir un pa\u00eds mejor.</span></span></li><li><span style=\"color:#6e6e6e;\">Trabajar con sus compa\u00f1eros para plantear estrategias concretas para la construcci\u00f3n de un pa\u00eds con esperanza y compromiso social.</span></li></ul>",
          "omitFocus": "false",
          "shortView": "view_529-001",
          "shortLook": "look_001"
        },
        "resourceProperties": [],
        "htmlResourceProperties": { "textContent": [] },

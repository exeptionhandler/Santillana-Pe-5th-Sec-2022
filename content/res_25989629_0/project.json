      "95c15a644987e095a9db722a2c4926e6": {
        "id": "95c15a644987e095a9db722a2c4926e6",
        "name": "text",
        "htmlDataRoot": false,
        "readOnly": false,
        "parent": "45907260ed0ad5dc98341849872de417",
        "properties": {
          "view": "text_view_529-001",
          "look": "text_view_529-001_look_001",
          "liColor": "",
          "textContent": "<p><span style=\"color:#ffffff;\">Al tener la ciudadan\u00eda de un pa\u00eds, se reciben algunos derechos y se aceptan algunos deberes.</span></p><p><span style=\"color:#ffffff;\">Todos tenemos el deber de trabajar por el bien com\u00fan.&nbsp;</span></p>",
          "omitFocus": "false"

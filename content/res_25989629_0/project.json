      },
      "a0224892b41383d8be83ffd8036ba8ce": {
        "id": "a0224892b41383d8be83ffd8036ba8ce",
        "name": "teacherContent",
        "htmlDataRoot": false,
        "readOnly": false,
        "parent": "ac7e1ad70a4349956979c754e39243b5",
        "properties": {
          "view": "teacherContent_view_529-001",
